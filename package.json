{
  "name": "qiita-stocker-nuxt",
  "version": "1.0.0",
  "description": "Mindexer Nuxt.js project",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development NUXT_HOST=127.0.0.1 NUXT_PORT=8080 nodemon",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node app/server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "format": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "createEnvrc:local": "DEPLOY_STAGE=local ts-node --project tsconfig.server.json createEnvrc.ts",
    "createEnvrc:dev": "DEPLOY_STAGE=dev ts-node --project tsconfig.server.json createEnvrc.ts",
    "createEnvrc:stg": "DEPLOY_STAGE=stg ts-node --project tsconfig.server.json createEnvrc.ts",
    "createEnvrc:prod": "DEPLOY_STAGE=prod ts-node --project tsconfig.server.json createEnvrc.ts"
  },
  "dependencies": {
    "@nuxtjs/bulma": "^1.2.3",
    "@nuxtjs/markdownit": "^1.2.4",
    "axios": "^0.18.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^2.6.1",
    "universal-cookie": "^3.1.0",
    "uuid": "^3.3.2",
    "vue-property-decorator": "^8.1.0"
  },
  "devDependencies": {
    "@nekonomokochan/aws-env-creator": "^2.0.0",
    "@nuxt/typescript": "^2.6.1",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/jest": "^24.0.11",
    "@types/universal-cookie": "^2.2.0",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.4.1",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.1.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.3",
    "vue-jest": "^3.0.4"
  }
}
