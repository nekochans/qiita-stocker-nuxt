{
  "name": "qiita-stocker-nuxt",
  "version": "1.0.0",
  "description": "Mindexer Nuxt.js project",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development NUXT_HOST=127.0.0.1 NUXT_PORT=8080 nuxt",
    "dev:server": "cross-env NODE_ENV=development nodemon",
    "build": "rm -rf .nuxt dist && tsc -p tsconfig.server.json && nuxt build",
    "start": "cross-env NODE_ENV=production node dist/server/server.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "format": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "deploy:dev": "yarn build && STAGE=dev NODE_ENV=production sls deploy -v",
    "deploy:stg": "yarn build && STAGE=stg NODE_ENV=production sls deploy -v",
    "deploy:prod": "yarn build && STAGE=prod NODE_ENV=production sls deploy -v",
    "remove:dev": "STAGE=dev sls remove -v",
    "remove:stg": "STAGE=stg sls remove -v",
    "remove:prod": "STAGE=prod sls remove -v",
    "createEnvrc:local": "DEPLOY_STAGE=local ts-node --project tsconfig.server.json createEnvrc.ts",
    "createEnvrc:dev": "DEPLOY_STAGE=dev ts-node --project tsconfig.server.json createEnvrc.ts",
    "createEnvrc:stg": "DEPLOY_STAGE=stg ts-node --project tsconfig.server.json createEnvrc.ts",
    "createEnvrc:prod": "DEPLOY_STAGE=prod ts-node --project tsconfig.server.json createEnvrc.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@nuxtjs/bulma": "^1.2.3",
    "@nuxtjs/markdownit": "^1.2.5",
    "aws-serverless-express": "^3.3.6",
    "axios": "^0.19.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "nuxt": "^2.6.1",
    "nuxt-property-decorator": "^2.3.0",
    "serverless-apigw-binary": "^0.4.4",
    "universal-cookie": "^4.0.0",
    "uuid": "^3.3.2",
    "vuex-type-helper": "^1.2.2"
  },
  "devDependencies": {
    "@nekonomokochan/aws-env-creator": "^2.0.0",
    "@nuxt/typescript": "^2.6.1",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@types/aws-serverless-express": "^3.3.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.13",
    "@types/universal-cookie": "^2.2.0",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.4.1",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": ">=4.1.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "serverless": "^1.46.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.3",
    "vue-jest": "^3.0.4"
  }
}
